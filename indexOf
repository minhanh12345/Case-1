<script>
    let arr = [1, 2, 3, 4, 5, 6]

    function indexOf(a) {
       for (let i=0;i<arr.length;i++){
           if (arr[i]==a){
               document.write(i)
           }
       }

    }
    indexOf(4)
</script>
