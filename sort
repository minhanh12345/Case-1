<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
    let arr = [2, 56, 32, 78, 1]

    function Sort() {
        for (let i=0;i<arr.length;i++){
            for(let j=i+1;j<arr.length;j++){
                if(arr[i]<arr[j]){
                    let a=arr[i]
                    arr[i]=arr[j]
                    arr[j]=a
                }
            }
        }
          document.write(arr)
    }

    Sort()
</script>
</body>
</html>
